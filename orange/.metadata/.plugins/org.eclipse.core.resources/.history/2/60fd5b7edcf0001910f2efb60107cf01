package webCrawling;

import java.io.IOException;

import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;
import org.jsoup.select.Elements;

public class kfhi {
	public static void main(String[] args) {
		
		//크롤링 할 페이지 url 지정
		String url = "https://kfhi.or.kr/apps/support/products.asp?";
		String selector = "div.boostList>ul";
        Document doc = null;
        
        try {
            doc = Jsoup.connect(url).get(); // 1. get방식의 URL에 연결해서 가져온 값을 doc에 담는다.
        } catch (IOException e) {
            System.out.println(e.getMessage());
        }
        
        //메인 요소 선택
	    Elements kfhi = doc.select(selector);
	    
	    //gfound title
	    String[] titles = kfhi.select("li>div.info").html().split("\n");
	    for(int t=0; t<titles.length; t++) {
	    	System.out.println(titles[t]);
	    }
	    
	}
}
